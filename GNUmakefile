SHELL := $(shell which bash)
export SHELL

HOSTNAME := $(shell uname -n)
export HOSTNAME

HOST_OS := $(shell uname -s)
export HOST_OS

BUILD_DIR := build
export BUILD_DIR

LOG4CXX_CONFIGURATION := ${BUILD_DIR}/run/log4j-config.xml
export LOG4CXX_CONFIGURATION

# include common part
include Makefile
