<setting>
	
	<entry       name      ="GVWin">
		<display width     ="1024"
		 	     height    ="640" />
		<file    disk      ="data/GVWin/GVWIN%1.DSK"
			     germ      ="data/GVWin/GVWIN.GRM"
			     boot      ="data/GVWin/NSINSTLR.DAT"
			     floppy    ="tmp/floppy/image/floppy144" />
		<boot    switch    ="Oy{|}\346\347\350\377"
				 device    ="DISK" />
		<memory  vmbits    ="25"
				 rmbits    ="24" />
		<network interface ="ens224" />
	</entry>
	
	<entry        name      ="Dawn">
		<display width     ="1024"
		 	     height    ="640" />
		<file    disk      ="data/Dawn/Dawn.dsk"
			     germ      ="data/Dawn/Dawn.germ"
			     boot      ="data/GVWin/NSINSTLR.DAT"
			     floppy    ="tmp/floppy/image/floppy144" />
		<boot    switch    ="8Wy{|}\346\347\350\377"
				 device    ="DISK" />
		<memory  vmbits    ="23"
				 rmbits    ="22" />
		<network interface ="ens224" />
	</entry>
	
	
